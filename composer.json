{
	"name": "silverstripe/asset-admin",
	"description": "Asset management for the SilverStripe CMS",
	"type": "silverstripe-module",
	"require": {
		"php": ">=5.5.0",
		"silverstripe/cms": "^4",
		"silverstripe/framework": "^4"
	},
	"require-dev": {
		"phpunit/PHPUnit": "^3.7"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"autoload": {
		"classmap": ["tests/behat/"]
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
